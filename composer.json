{
  "name": "rawr/explain",
  "description": "Allows debug, analyze and simplify complex regular expressions.",
  "license": "MIT",
  "type": "library",
  "autoload": {
    "psr-4": {
      "TRegx\\": "src/TRegx"
    }
  },
  "autoload-dev": {
    "classmap": [
      "test/"
    ],
    "psr-4": {
      "Test\\": "test/"
    }
  },
  "require": {
    "php": ">=7.1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "archive": {
    "exclude": [
      "/test/**",
      "/test"
    ]
  }
}
